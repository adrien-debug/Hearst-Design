# Application
NODE_ENV=development
PORT=3003

# Supabase Configuration
# ⚠️ REMPLACER avec vos vraies clés Supabase
# Récupérer dans: https://supabase.com/dashboard > Settings > API
SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.YOUR_ANON_KEY
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.YOUR_SERVICE_KEY

# JWT Configuration
# ⚠️ GÉNÉRER avec: openssl rand -base64 32
JWT_SECRET=GENERATE_NEW_SECRET_WITH_OPENSSL_RAND_BASE64_32
JWT_EXPIRES_IN=24h

# CORS Configuration
CORS_ORIGIN=http://localhost:3100

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=debug
